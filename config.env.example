# CTyun ZOS SDK Configuration
# Copy this file to .env and fill in your actual values

# Required: Your CTyun access key ID
S3_ACCESS_KEY=your_access_key_here

# Required: Your CTyun secret access key  
S3_SECRET_KEY=your_secret_key_here

# Required: CTyun region (e.g., huabei-2, huadong-1, huadong-2, huanan-1, huanan-2)
S3_REGION=huabei-2

# Required: CTyun ZOS endpoint URL
S3_ENDPOINT=https://huabei-2.zos.ctyun.cn

# Optional: Bucket name for testing (you can override this in your code)
S3_BUCKET=your_bucket_name

# Optional: SSL verification (set to false if you have SSL issues)
S3_VERIFY_SSL=true

# Optional: Request timeout in seconds
S3_TIMEOUT=30
